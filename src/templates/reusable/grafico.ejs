<div id="grafico">
    <div class="celula">
      <div class="text">ejso</div>
      <div class="barra" style="position: relative; left: -2px; background-color: #0088ff; width: <%= parseInt( alunos_fundamental)/parseInt(total_aluno) *100%>%;"></div><span class="total_barra"><%= alunos_fundamental %> </span>
    </div>
    <hr>
    <div class="celula">
      <div class="text" >Infantil</div>
      <div class="barra" style="background-color: #399DF5; width: <%= parseInt( alunos_infantil)/parseInt(total_aluno)*100 %>%; margin-left: 100px;"></div><span class="total_barra"><%= alunos_infantil %> </span>
    </div>
    <hr>
    <div class="celula">
      <div class="text">EJA</div> 
      <div class="barra" style="background-color: #6AB6F7; width: <%= parseInt(alunos_eja)/parseInt(total_aluno) *100%>%; margin-left: 130px;"></div><span class="total_barra"><%=alunos_eja%></span>
    </div>
    <hr color="black">
    <div id="legenda">
        <ul>
          <li style="margin-left: 0px; position: relative; left: -5px;">0</li><div style=" position: relative; left: -12px; top: -240px; height: 240px; border-left: 1px solid black;"></div>
          <li><%= (total_aluno / 5).toFixed() %> </li><div style="z-index: -1; position: relative; left: -20px; top: -240px; height: 240px; border-left: 1px solid rgba(0, 0, 0, 0.438);"></div>
          <li><%= (total_aluno / 4).toFixed() %> </li><div style="z-index: -1; position: relative; left: -20px; top: -240px; height: 240px; border-left: 1px solid rgba(0, 0, 0, 0.438);"></div>
          <li><%= (total_aluno / 3).toFixed() %> </li><div style="z-index: -1; position: relative; left: -20px; top: -240px; height: 240px; border-left: 1px solid rgba(0, 0, 0, 0.438);"></div>
          <li><%= (total_aluno / 2).toFixed() %> </li><div style="z-index: -1; position: relative; left: -20px; top: -240px; height: 240px; border-left: 1px solid rgba(0, 0, 0, 0.438);"></div>
          <li style="position: relative; left: 25px;"><%= total_aluno %> </li><div style="z-index: -1; position: relative; top: -240px; height: 240px; border-left: 1px solid rgba(0, 0, 0, 0.438);"></div>
        </ul>
    </div>
  </div> 